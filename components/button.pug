if activate == 'on'
  div(id=content_id class="ui " +  color + " button" onClick="rpcServer.click_" + id + "(gameboard.clientId);"  style="position:absolute; top:" + y + "; left:" + x + ";") #{label}
else
  div(id=content_id class="ui disabled" +  color + " button" style="position:absolute; top:" + y + "; left:" + x + ";") #{label}
